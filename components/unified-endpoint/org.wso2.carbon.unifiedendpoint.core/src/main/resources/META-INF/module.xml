<module name="UEPModule" class="org.wso2.carbon.unifiedendpoint.core.UnifiedEndpointModule">
    <Description>This module is an implementation of Unified Endpoints.</Description>
    <OutFlow>
        <handler name="OutFlowUEPHandler" class="org.wso2.carbon.unifiedendpoint.core.UnifiedEndpointHandler">
            <order phase="UEPPhase"/>
        </handler>
    </OutFlow>

    <OutFaultFlow>
        <handler name="FaultOutFlowUEPHandler" class="org.wso2.carbon.unifiedendpoint.core.UnifiedEndpointHandler">
            <order phase="UEPPhase"/>
        </handler>
    </OutFaultFlow>
</module>

